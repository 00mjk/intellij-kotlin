fun test1() : Boolean {
    try {
        return true
    } finally {
          if(true) // otherwise we wisely have unreachable code
            return false
    }
}

var x = true
fun test2() : Boolean {
    try {
    } finally {
        x = false;
    }
    return x
}

fun test3() : Int {
    var y = 0
    try {
        ++y
    } finally {
        ++y
    }
    return y
}

var z = 0
fun test4() : Int {
    z = 0
    return try {
        try {
            z++
        }
        finally {
            z++
        }
    } finally {
        ++z
    }
}

fun box() : String {
    if(test1()) return "test1 failed"
    if(test2()) return "test2 failed"
    if(test3() != 2) return "test3 failed"
    System.out?.println(test4())
    if(test4() != 3) return "test4 failed"

    return "OK"
}
