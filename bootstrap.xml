<project name="Jet CI Bootstrap" default="unzipIdeaSDK">
    <target name="unzipIdeaSDK">
        <unzip dest="ideaSDK">
            <fileset dir="ideaSDK" includes="ideaIC*.zip"/>
        </unzip>
        <echo file="idea/idea.properties" message="idea.home=${basedir}/ideaSDK"/>
    </target>
</project>
